        "Blotter": {
            "window": {
                "url": "http://192.168.1.83:3000/blotter",
                "frame": false,
                "resizable": true,
                "autoShow": true,
                "top": "center",
                "left": "center",
                "width": 1000,
                "height": 1000
            },
            "component": {
                "inject": [
                    "http://192.168.1.83:3000/blotter"
                ]
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "title": "Blotter",
                        "FSBLHeader": true,
                        "persistWindowState": false
                    },
                    "Toolbar": {
                        "iconClass": "blotter"
                    }
                }
            }
        },
