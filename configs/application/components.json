        "Currency Chart": {
            "window": {
                "url": "$applicationRoot/components/currencyChart/technical-analysis-chart.html",
                "frame": false,
                "resizable": true,
                "autoShow": true,
                "top": "center",
                "left": "center",
                "width": 1000,
                "height": 900
            },
            "component": {
                "inject": [
                    "$applicationRoot/components/mindcontrol/chartSubscribeLinker.js",
                    "$applicationRoot/components/mindcontrol/chartDragAndDropReceiver.js"
                ]
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "title": "Currency Chart",
                        "FSBLHeader": true,
                        "hackScrollbar": true,
                        "persistWindowState": true,
                        "showLinker": true
                    },
                    "Toolbar": {
                        "iconClass": "CurrencyChart"
                    }
                }
            }
        },
