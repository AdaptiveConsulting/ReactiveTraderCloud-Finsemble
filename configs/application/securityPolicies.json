{
	"securityPolicies": {
		"comment": "Only Supported on electron",
		"//trusted": "By default, the trusted policy allows access to every API endpoint.",
		"trusted": {
			"Window": {
				"webPreferences": {
					"preload": true
				}
			}
		},
		"untrusted": {
			"System": {
				"clearCache": false,
				"exit": false,
				"launchExternalProcess": false
			},
			"Window": {
				"executeJavaScript": false,
				"webPreferences": {
					"preload": false
				}
			}
		}
	},
	"securityPolicyRules": {
		"crossDomain": "untrusted",
		"sameDomain": "trusted"
	}
}
