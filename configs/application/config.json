{
	"comment": "this file defines application-level config for development, adding on top of the core config",
	"devMode": false,
	"isAuthEnabled": false,
	"defaultStorage": "localStorage",
	"storage": {},
	"betaFeatures": {
		"docking": {
			"enabled": false
		},
		"assimilation": {
			"enabled": false,
			"mode": "whitelist",
			"whitelist": {},
			"blacklist": {},
			"appAssets": [
				{
					"comment": "Necessary application for assimilation to work.",
					"src": "/hosted/assimilation.zip",
					"version": "1.0.2",
					"alias": "assimilation",
					"target": "CppWindowsHook.exe"
				},
				{
					"comment": "Example application. Replace the contents of this object with your own zip file.",
					"src": "/hosted/putty.zip",
					"version": "1.0.0",
					"alias": "putty",
					"target": "putty.exe"
				}
			]
		}
	},
	"importConfig": [
		"configs/components.json",
		"configs/finsembleComponents.json",
		"configs/services.json",
		"configs/defaultWorkspaces.json"
	]
}