{
	"devtools_port": 9090,
	"startup_app": {
		"name": "Reactive Trader Finsemble Production",
		"url": "https://storage.googleapis.com/reactive-trader-finsemble/finsemble/services/systemManager/systemManager.html",
		"uuid": "Finsemble-Prod",
		"applicationIcon": "https://storage.googleapis.com/reactive-trader-finsemble/assets/img/adaptive-logo@2x.png",
		"defaultTop": 0,
		"defaultLeft": 0,
		"showTaskbarIcon": true,
		"autoShow": false,
		"frame": false,
		"resizable": false,
		"maximizable": false,
		"delay_connection": true,
		"contextMenu": true,
		"cornerRounding": {
			"width": 4,
			"height": 4
		},
		"alwaysOnTop": false
	},
	"splashScreenImage": "https://storage.googleapis.com/reactive-trader-finsemble/assets/img/splash-screen.jpg",
	"runtime": {
		"arguments": "--noerrdialogs  --v=4 --no-sandbox  --framestrategy=frames --winhttp-proxy-resolver",
		"version": "9.61.36.36",
		"//": "Add these arguments when diagnosing issues",
		"--enable-crash-reporting": "Adding this flag will enable crash reporting to openfin, which has caused memory leaks in some versions.",
		"--diagnostics": "Adding this to the runtimeArguments activates verbose logging and crash reporting.",
		"--disable-gpu": "This flag disables hardware acceleration. Try this if 'Blank Screen' issues are reported (sometimes on virtual desktops)",
		"--winhttp-proxy-resolver": "Chromium v56 contains a bug that in some environments can create a memory leak. The bug is related to the way Chromium handles PAC files."
	},
	"shortcut": {
		"company": "Adaptive",
		"description": "Reactice Trader Finsemble Production",
		"icon": "https://storage.googleapis.com/reactive-trader-finsemble/assets/img/favicon.ico",
		"name": "Adaptive - PPROD",
		"target": ["desktop", "start-menu"],
		"force": false,
		"startMenuRootFolder": "Reactice Trader Finsemble Production"
	},
	"dialogSettings": {
		"logo": "https://storage.googleapis.com/reactive-trader-finsemble/assets/img/ciq-banner-100x25.png",
		"bgColor": 4280109424,
		"textColor": 4293521652,
		"progressBarBgColor": 4294967295,
		"progressBarFillColor": 4282684881,
		"progressBarBorderColor": 4293521652
	},
	"supportInformation": {
		"company": "Adaptive",
		"product": "Reactice Trader Finsemble Production",
		"email": "support@weareadaptive.com"
	},
	"fileName": "ReacticeTraderFinsemble-Prod",
	"appAssets": [
		{
			"src": "https://assets.finsemble.com/assimilation/assimilation_4.0.0.0.zip",
			"version": "4.0.0",
			"alias": "assimilation",
			"target": "AssimilationMain.exe"
		},
		{
			"src": "https://storage.googleapis.com/reactive-trader-finsemble/assets/installer/limitchecker/LimitChecker.zip",
			"alias": "LimitChecker",
			"version": "1.1.0",
			"target": "LimitChecker.exe"
		}
	],
	"finsemble": {
		"applicationRoot": "https://storage.googleapis.com/reactive-trader-finsemble",
		"moduleRoot": "$applicationRoot/finsemble",
		"componentRoot": "https://web-dev.adaptivecluster.com",
		"servicesRoot": "$applicationRoot/finsemble/services",
		"notificationURL": "$applicationRoot/components/notification/notification.html",
		"bootConfig": {
			"onErrorMakeSystemManagerVisible": true,
			"defaults": {
				"startServiceTimeout": 10000,
				"startComponentTimeout": 15000,
				"startTaskTimeout": 10000
			}
		},
		"importConfig": ["$applicationRoot/configs/application/config.json"],
		"router": {
			"crossDomainTransport": "IPCBus",
			"transportSettings": {
				"FinsembleTransport": {
					"serverAddress": "ws://127.0.0.1:3376"
				}
			}
		}
	}
}
